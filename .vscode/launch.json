    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Django",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/manage_mo.py",
            "python": "${workspaceFolder}/venv/bin/python",
            "args": [
                "runserver",
                "0.0.0.0:9071"
            ],
            "django": true,
            "autoStartBrowser": false,
            "justMyCode": false,
            // "env": {
            //     "LEDGER_API_URL": "http://10.17.0.10:9021",
            // }
        }
    ]
}